function string Standalone.getModdedAnimationSpriteKey(u8 character, u16 animationSpriteEx)
{
	if (isECFCharA0("ECC-Amy") && animationSpriteEx >= 0x21 && animationSpriteEx <= 0x30 && (global.zone == 0x0c || global.zone == 0x0d))
		return stringformat("character_ECC-Amy_superfly_%d", ((animationSpriteEx - 0x21) % 2) + ((animationSpriteEx - 0x21) / 4) * 2)
	return base.Standalone.getModdedAnimationSpriteKey(character, animationSpriteEx)
}